$Animation "Run_com_Front" 			"new_anims/locomotion/run/stand/stand_run_com_fwd_fast.smd" 			fps 45	loop LX LY	 rotateto 	0
$Animation "Run_com_Front_left" 	"new_anims/locomotion/run/stand/stand_run_com_fwd_fast.smd" 			fps 45	loop LX LY	 rotateto 	45
$Animation "Run_com_Front_right" 	"new_anims/locomotion/run/stand/stand_run_com_fwd_fast.smd" 			fps 45	loop LX LY	 rotateto  -45
$Animation "Run_com_Back" 			"new_anims/locomotion/run/stand/stand_run_com_bwd_fast.smd" 			fps 45	loop LX LY	 rotateto  -180
$Animation "Run_com_Back_left" 		"new_anims/locomotion/run/stand/stand_run_com_bwd_fast.smd" 			fps 45	loop LX LY	 rotateto 	135
$Animation "Run_com_Back_right" 	"new_anims/locomotion/run/stand/stand_run_com_bwd_fast.smd" 			fps 45	loop LX LY	 rotateto  -135
$Animation "Run_com_Right" 			"new_anims/locomotion/run/stand/stand_runstrafefwd_com_rgt_fast.smd" 	fps 45	loop LX LY	 rotateto  -90
$Animation "Run_com_Left" 			"new_anims/locomotion/run/stand/stand_runstrafefwd_com_lft_fast.smd" 	fps 45	loop LX LY	 rotateto 	90

$sequence "stand_run_com" {
	"Run_com_Back"
    "Run_com_Back_right"
	"Run_com_Right"
	"Run_com_Front_right"
	"Run_com_Front"
	"Run_com_Front_left"
	"Run_com_Left"
	"Run_com_Back_left"
	"Run_com_Back"
    blendwidth 9 blend move_yaw -180 180
	activity "ACT_RUN" 1
	addlayer "aimrun_lo_params"
}
