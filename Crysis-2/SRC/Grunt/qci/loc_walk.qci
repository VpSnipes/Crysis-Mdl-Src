$Animation "Walk_com_Front" 		"anims/locomotion/walk/stand/stand_walk_com_fwd_fast.smd" 				loop LX LY	 rotateto 	0
$Animation "Walk_com_Front_left" 	"anims/locomotion/walk/stand/stand_walk_com_fwd_fast.smd" 				loop LX LY	 rotateto 	45
$Animation "Walk_com_Front_right" 	"anims/locomotion/walk/stand/stand_walk_com_fwd_fast.smd" 				loop LX LY	 rotateto  -45
$Animation "Walk_com_Back" 			"anims/locomotion/walk/stand/stand_walk_com_bwd_fast.smd" 				loop LX LY	 rotateto  -180
$Animation "Walk_com_Back_left" 	"anims/locomotion/walk/stand/stand_walk_com_bwd_fast.smd" 				loop LX LY	 rotateto 	135
$Animation "Walk_com_Back_right" 	"anims/locomotion/walk/stand/stand_walk_com_bwd_fast.smd" 				loop LX LY	 rotateto  -135
$Animation "Walk_com_Right" 		"anims/locomotion/walk/stand/stand_walkstrafefwd_com_rgt_fast.smd" 		loop LX LY	 rotateto  -90
$Animation "Walk_com_Left" 			"anims/locomotion/walk/stand/stand_walkstrafefwd_com_lft_fast.smd" 		loop LX LY	 rotateto 	90

$sequence "stand_walk_com" {
	"Walk_com_Back"
    "Walk_com_Back_right"
	"Walk_com_Right"
	"Walk_com_Front_right"
	"Walk_com_Front"
	"Walk_com_Front_left"
	"Walk_com_Left"
	"Walk_com_Back_left"
	"Walk_com_Back"
    blendwidth 9 blend move_yaw -180 180
	activity "ACT_WALK" 1
	addlayer "aim_move_com"
	addlayer "aimwalk_hi_params"
}

$Animation "Walk_Front" 		"anims/locomotion/walk/stand/stand_walk_idle_fwd_fast.smd" 				loop LX LY	 rotateto 	0
$Animation "Walk_Front_left" 	"anims/locomotion/walk/stand/stand_walk_idle_fwd_fast.smd" 				loop LX LY	 rotateto 	45
$Animation "Walk_Front_right" 	"anims/locomotion/walk/stand/stand_walk_idle_fwd_fast.smd" 				loop LX LY	 rotateto  -45
$Animation "Walk_Back" 			"anims/locomotion/walk/stand/stand_walk_idle_bwd_fast.smd" 				loop LX LY	 rotateto  -180
$Animation "Walk_Back_left" 	"anims/locomotion/walk/stand/stand_walk_idle_bwd_fast.smd" 				loop LX LY	 rotateto 	135
$Animation "Walk_Back_right" 	"anims/locomotion/walk/stand/stand_walk_idle_bwd_fast.smd" 				loop LX LY	 rotateto  -135
$Animation "Walk_Right" 		"anims/locomotion/walk/stand/stand_walkstrafefwd_idle_rgt_fast.smd" 	loop LX LY	 rotateto  -90
$Animation "Walk_Left" 			"anims/locomotion/walk/stand/stand_walkstrafefwd_idle_lft_fast.smd" 	loop LX LY	 rotateto 	90

$sequence "stand_walk" {
	"Walk_Back"
    "Walk_Back_right"
	"Walk_Right"
	"Walk_Front_right"
	"Walk_Front"
	"Walk_Front_left"
	"Walk_Left"
	"Walk_Back_left"
	"Walk_Back"
    blendwidth 9 blend move_yaw -180 180
	activity "ACT_WALK_RELAXED" 1
	addlayer "aimwalk_lo_params"
}
